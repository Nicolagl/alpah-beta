/**index.wxss**/

.swiper-image {
  width: 100%;
}

.nav-item {
  width: 50%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding: 20rpx;
}

.nav-img {
  width: 110rpx;
  height: 114rpx;
}
.nav-text{
  padding-top: 20rpx;
  font-size: 30rpx;
}
.category-title{
  padding-top: 15rpx;
  padding-left: 20rpx;
  padding-bottom: 15rpx;
  display: flex;
  align-items: center;
}
.small-icon-img{
  width: 45rpx;
  height:45rpx;
  margin-right: 15rpx;
}
.small-text{
  font-size: 30rpx;
   font-weight: 900;
}

.round-click{
    height: 150rpx;
    width: 150rpx;
    background-color: #d92a2a;
    border-radius: 100%;
    position: fixed;
    bottom: 150rpx;
    top: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 9;
}
.round-click navigator{
    font-size: 32rpx;
    max-width: 80rpx;
    color: #fff;
    text-align: center;
}